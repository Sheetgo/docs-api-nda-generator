{
  "env": {
    "browser": true,
    "googleappsscript/googleappsscript": true
  },
  "parserOptions": {
    "ecmaVersion": 5
  },
  "rules": {
    "block-spacing": 1,
    "brace-style": 1,
    "comma-spacing": [1, { "before": false, "after": true }],
    "comma-style": 1,
    "key-spacing": 1,
    "keyword-spacing": 1,
    "no-multi-spaces": 1,
    "no-undef": 0,
    "no-unused-vars": 0,
    "semi": ["error","never"],
    "space-before-blocks" : [1, "always"]
  },
  "globals": {
    "Docs": true
  },
  "plugins": [
    "googleappsscript"
  ],
  "extends": "eslint:recommended"
}
